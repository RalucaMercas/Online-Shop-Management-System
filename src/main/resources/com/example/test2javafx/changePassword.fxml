<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="285.0" prefWidth="326.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test2javafx.ChangePasswordController">
    <children>
        <Label layoutX="24.0" layoutY="67.0" text="Old password">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="textOldPassword" layoutX="134.0" layoutY="64.0" />
        <Label layoutX="21.0" layoutY="118.0" text="New password">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="textNewPassword" layoutX="134.0" layoutY="115.0" />
        <Button fx:id="buttonSaveChanges" layoutX="109.0" layoutY="180.0" mnemonicParsing="false" onAction="#handleSaveChanges" text="Save changes">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
